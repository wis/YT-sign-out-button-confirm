{
  "manifest_version": 3,
  "name": "YouTube Sign Out Confirm",
  "version": "1.0",
  "description": "Asks for confirmation before signing out of YouTube.",
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "assets/icons/16x16.png",
    "24": "assets/icons/24x24.png",
    "32": "assets/icons/32x32.png",
    "128": "assets/icons/128x128.png"
  }
}
